# Copiez ce fichier en ".env" et renseignez vos valeurs privées
# Ne commitez jamais le fichier .env

# Clé API Mistral (obligatoire)
MISTRAL_API_KEY=

# Modèle (optionnel selon usage du client)
MISTRAL_MODEL=mistral-small-latest

# Identifiant de l'agent Mistral (obligatoire, doit exister côté Mistral)
JARVIS_BASE_AGENT_ID=

# Forcer l'utilisation de l'Agent API Mistral
USE_MISTRAL_AGENT_API=1
